.SS "#define define_atouintN(N)"
\fBValue:\fP
.PP
.nf
uint ## N ## _t \
atouint ## N(const char* str) \
{ \
    char* endptr; \
    errno = 0; \
    uintmax_t val = strtoumax(str, &endptr, 10); \
    if (errno == ERANGE || val > UINT ## N ## _MAX \
        || endptr == str || *endptr != '\0') { \
        errno = ERANGE; \
        return 0; \
    } \
    return (uint ## N ## _t) val; \
}
.fi
